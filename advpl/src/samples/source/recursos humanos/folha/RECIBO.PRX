Static Function fRTipoEmp()

Local nAux:= 1       
Local cTpEmprega := ""
Local aTabTipoEmp := {}
    
fCarrTab( @aTabTipoEmp, "S029", Nil)
    
IF ( len(aTabTipoEmp) >= 1 )
    nAux := 1
	While ( nAux <= len(aTabTipoEmp) )               
        If ( aTabTipoEmp[nAux,1] == "S029" .And. aTabTipoEmp[nAux,5] == SRA->RA_TPFUNC )
    		cTpEmprega := Trim(aTabTipoEmp[nAux,6])
