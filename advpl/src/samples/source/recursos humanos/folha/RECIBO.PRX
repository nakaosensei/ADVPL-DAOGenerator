Local aTabDescAFP := {}
    
fCarrTab( @aTabDescAFP, "S004", Nil)
    
IF ( len(aTabDescAFP) >= 1 )
    nAux := 1
	While ( nAux <= len(aTabDescAFP) )               
        If ( aTabDescAFP[nAux,1] == "S004" .And. aTabDescAFP[nAux,5] == SRA->RA_CODAFP )
    		cDescAfp := Trim(aTabDescAFP[nAux,6])
    		nAux := LEN(aTabDescAFP)
    	Endif
    	nAux++
    End
Endif

Return cDescAfp

